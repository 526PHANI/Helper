{
    "sections": [
      {
        "title": "Basic Structure",
        "content": [
          {
            "description": "HTML Document Structure",
            "code": "<!DOCTYPE html>\n<html>\n<head>\n  <title>Page Title</title>\n</head>\n<body>\n  <h1>Heading</h1>\n  <p>Paragraph</p>\n</body>\n</html>"
          },
          {
            "description": "DOCTYPE Declaration",
            "code": "<!DOCTYPE html>"
          },
          {
            "description": "HTML Root Element",
            "code": "<html></html>"
          },
          {
            "description": "Head Section",
            "code": "<head>\n  <title>Page Title</title>\n  <meta charset=\"UTF-8\">\n  <meta name=\"description\" content=\"Page description\">\n</head>"
          },
          {
            "description": "Body Section",
            "code": "<body>\n  <!-- Content goes here -->\n</body>"
          }
        ]
      },
      {
        "title": "Text Formatting",
        "content": [
          {
            "description": "Headings (h1 to h6)",
            "code": "<h1>Heading 1</h1>\n<h2>Heading 2</h2>\n<h3>Heading 3</h3>"
          },
          {
            "description": "Paragraph",
            "code": "<p>This is a paragraph.</p>"
          },
          {
            "description": "Bold Text",
            "code": "<b>Bold Text</b> or <strong>Strong Text</strong>"
          },
          {
            "description": "Italic Text",
            "code": "<i>Italic Text</i> or <em>Emphasized Text</em>"
          },
          {
            "description": "Underlined Text",
            "code": "<u>Underlined Text</u>"
          },
          {
            "description": "Strikethrough Text",
            "code": "<s>Strikethrough Text</s>"
          },
          {
            "description": "Superscript",
            "code": "<sup>Superscript</sup>"
          },
          {
            "description": "Subscript",
            "code": "<sub>Subscript</sub>"
          },
          {
            "description": "Line Break",
            "code": "<br>"
          },
          {
            "description": "Horizontal Rule",
            "code": "<hr>"
          }
        ]
      },
      {
        "title": "Links and Images",
        "content": [
          {
            "description": "Hyperlink",
            "code": "<a href=\"https://example.com\">Visit Example</a>"
          },
          {
            "description": "Link to Email",
            "code": "<a href=\"mailto:example@example.com\">Send Email</a>"
          },
          {
            "description": "Link to Phone Number",
            "code": "<a href=\"tel:+1234567890\">Call Us</a>"
          },
          {
            "description": "Image",
            "code": "<img src=\"image.jpg\" alt=\"Description\">"
          },
          {
            "description": "Image with Width and Height",
            "code": "<img src=\"image.jpg\" alt=\"Description\" width=\"500\" height=\"300\">"
          }
        ]
      },
      {
        "title": "Lists",
        "content": [
          {
            "description": "Unordered List",
            "code": "<ul>\n  <li>Item 1</li>\n  <li>Item 2</li>\n</ul>"
          },
          {
            "description": "Ordered List",
            "code": "<ol>\n  <li>Item 1</li>\n  <li>Item 2</li>\n</ol>"
          },
          {
            "description": "Description List",
            "code": "<dl>\n  <dt>Term 1</dt>\n  <dd>Description 1</dd>\n  <dt>Term 2</dt>\n  <dd>Description 2</dd>\n</dl>"
          }
        ]
      },
      {
        "title": "Tables",
        "content": [
          {
            "description": "Basic Table",
            "code": "<table>\n  <tr>\n    <th>Header 1</th>\n    <th>Header 2</th>\n  </tr>\n  <tr>\n    <td>Data 1</td>\n    <td>Data 2</td>\n  </tr>\n</table>"
          },
          {
            "description": "Table with Border",
            "code": "<table border=\"1\">\n  <tr>\n    <th>Header 1</th>\n    <th>Header 2</th>\n  </tr>\n  <tr>\n    <td>Data 1</td>\n    <td>Data 2</td>\n  </tr>\n</table>"
          },
          {
            "description": "Table with Caption",
            "code": "<table>\n  <caption>Table Title</caption>\n  <tr>\n    <th>Header 1</th>\n    <th>Header 2</th>\n  </tr>\n  <tr>\n    <td>Data 1</td>\n    <td>Data 2</td>\n  </tr>\n</table>"
          },
          {
            "description": "Table with Colspan",
            "code": "<table>\n  <tr>\n    <th colspan=\"2\">Header</th>\n  </tr>\n  <tr>\n    <td>Data 1</td>\n    <td>Data 2</td>\n  </tr>\n</table>"
          },
          {
            "description": "Table with Rowspan",
            "code": "<table>\n  <tr>\n    <th rowspan=\"2\">Header</th>\n    <td>Data 1</td>\n  </tr>\n  <tr>\n    <td>Data 2</td>\n  </tr>\n</table>"
          }
        ]
      },
      {
        "title": "Forms",
        "content": [
          {
            "description": "Form with Input Fields",
            "code": "<form>\n  <label for=\"name\">Name:</label>\n  <input type=\"text\" id=\"name\" name=\"name\">\n  <input type=\"submit\" value=\"Submit\">\n</form>"
          },
          {
            "description": "Text Input",
            "code": "<input type=\"text\" name=\"text-input\">"
          },
          {
            "description": "Password Input",
            "code": "<input type=\"password\" name=\"password\">"
          },
          {
            "description": "Email Input",
            "code": "<input type=\"email\" name=\"email\">"
          },
          {
            "description": "Number Input",
            "code": "<input type=\"number\" name=\"number\">"
          },
          {
            "description": "Checkbox",
            "code": "<input type=\"checkbox\" name=\"checkbox\">"
          },
          {
            "description": "Radio Button",
            "code": "<input type=\"radio\" name=\"radio\">"
          },
          {
            "description": "Dropdown Menu",
            "code": "<select name=\"dropdown\">\n  <option value=\"1\">Option 1</option>\n  <option value=\"2\">Option 2</option>\n</select>"
          },
          {
            "description": "Textarea",
            "code": "<textarea name=\"textarea\"></textarea>"
          },
          {
            "description": "Submit Button",
            "code": "<input type=\"submit\" value=\"Submit\">"
          },
          {
            "description": "Reset Button",
            "code": "<input type=\"reset\" value=\"Reset\">"
          }
        ]
      },
      {
        "title": "Semantic Elements",
        "content": [
          {
            "description": "Header",
            "code": "<header>\n  <h1>Website Title</h1>\n</header>"
          },
          {
            "description": "Navigation",
            "code": "<nav>\n  <a href=\"/\">Home</a>\n  <a href=\"/about\">About</a>\n</nav>"
          },
          {
            "description": "Main Content",
            "code": "<main>\n  <p>Main content goes here.</p>\n</main>"
          },
          {
            "description": "Section",
            "code": "<section>\n  <h2>Section Title</h2>\n  <p>Section content.</p>\n</section>"
          },
          {
            "description": "Article",
            "code": "<article>\n  <h2>Article Title</h2>\n  <p>Article content.</p>\n</article>"
          },
          {
            "description": "Aside",
            "code": "<aside>\n  <p>Sidebar content.</p>\n</aside>"
          },
          {
            "description": "Footer",
            "code": "<footer>\n  <p>Footer content.</p>\n</footer>"
          }
        ]
      },
      {
        "title": "Multimedia",
        "content": [
          {
            "description": "Audio",
            "code": "<audio controls>\n  <source src=\"audio.mp3\" type=\"audio/mpeg\">\n  Your browser does not support audio.\n</audio>"
          },
          {
            "description": "Video",
            "code": "<video controls>\n  <source src=\"video.mp4\" type=\"video/mp4\">\n  Your browser does not support video.\n</video>"
          },
          {
            "description": "Embed YouTube Video",
            "code": "<iframe width=\"560\" height=\"315\" src=\"https://www.youtube.com/embed/VIDEO_ID\" frameborder=\"0\" allowfullscreen></iframe>"
          }
        ]
      },
      {
        "title": "Meta Tags",
        "content": [
          {
            "description": "Character Encoding",
            "code": "<meta charset=\"UTF-8\">"
          },
          {
            "description": "Viewport for Responsive Design",
            "code": "<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">"
          },
          {
            "description": "Page Description",
            "code": "<meta name=\"description\" content=\"Page description\">"
          },
          {
            "description": "Keywords",
            "code": "<meta name=\"keywords\" content=\"HTML, CSS, JavaScript\">"
          },
          {
            "description": "Author",
            "code": "<meta name=\"author\" content=\"Author Name\">"
          }
        ]
      }
    ]
  }